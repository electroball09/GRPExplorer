<Window x:Class="GRPExplorerGUI.Extra.FEUtoSWFWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GRPExplorerGUI.Extra"
        mc:Ignorable="d"
        Title="FEUtoSWFWindow" Height="123.088" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" AllowDrop="True" Drop="txtFileName_Drop">
            <Label x:Name="label" Content="File:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
            <TextBox x:Name="txtFileName" HorizontalAlignment="Left" Height="23" Margin="0,14,0,0" TextWrapping="Wrap" Text="" IsReadOnly="True" AllowDrop="True" Drop="txtFileName_Drop" VerticalAlignment="Top" Width="743"/>

        </StackPanel>
        <Button x:Name="button" Content="Convert" HorizontalAlignment="Left" Click="button_Click" Margin="46,42,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="btnOpenFile" Content="Open file..." Margin="659,42,10,24" Click="btnOpenFile_Click"/>
    </Grid>
</Window>
